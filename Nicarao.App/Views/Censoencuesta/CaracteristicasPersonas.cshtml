                        <label class="has-float-label" data-toggle="tooltip" data-placement="top" title="Carrera técnica o universitaria">
                            <select id="ddlCarreraTecnicaUniversitaria" class="form-control sel"></select>
                        </label>
                    </div>
                </div>
                <div class="col-md-2">
                    <div class="form-group">
                        <label class="has-float-label" data-toggle="tooltip" data-placement="top" title="Completo Carrera técnica o universitaria">
                            <select id="ddlCompletoCarreraTecnicaUniversitaria" class="form-control sel"></select>
