                        <label class="has-float-label" data-toggle="tooltip" data-placement="top" title="Religión">
                            <select id="ddlReligion" class="form-control sel"></select>
                        </label>
                    </div>
                </div>
                <div class="col-md-2">
                    <div class="form-group">
                        <label class="has-float-label" data-toggle="tooltip" data-placement="top" title="Cuál">
                            @Html.TextBoxFor(model => model.Identificador, new { id = "txtCualReligion", PlaceHolder = "Cuál", @class = "form-control", disabled = "disabled", autocomplete = "off" })
                            <span>Cuál</span>
                        </label>
                    </div>
                </div>
                <div class="col-md-2">
                    <div class="form-group">
                        <label class="has-float-label" data-toggle="tooltip" data-placement="top" title="Dependiente del hogar">
                            <select id="ddlDependienteHogar" class="form-control sel"></select>
                        </label>
                    </div>
                </div>
                <div class="col-md-2">
                    <div class="form-group">
                        <label class="has-float-label" data-toggle="tooltip" data-placement="top" title="Razón Dependiente del hogar">
                            <select id="ddlRazonDependienteHogar" class="form-control sel"></select>
                        </label>
                    </div>
                </div>
                <div class="col-md-2">
                    <div class="form-group">
                        <label class="has-float-label" data-toggle="tooltip" data-placement="top" title="Cuál">
                            @Html.TextBoxFor(model => model.Identificador, new { id = "txtCualRazonDepediente", PlaceHolder = "Cuál", @class = "form-control", disabled = "disabled", autocomplete = "off" })
                            <span>Cuál</span>
                        </label>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <label>¿En qué lugar del país nacio?</label>
                </div>
                <div class="col-md-4"></div>
            </div>
            <div class="row">
                <div class="col-md-2">
                    <div class="form-group">
                        <label class="has-float-label" data-toggle="tooltip" data-placement="top" title="Departamento">
                            <select id="ddlDepartamento" class="form-control sel"></select>
                            <span>Departamento</span>
                        </label>
                    </div>
                </div>
                <div class="col-md-2">
                    <div class="form-group">
                        <label class="has-float-label" data-toggle="tooltip" data-placement="top" title="Municipio">
                            <select id="ddlMunicipio" class="form-control sel"></select>
                            <span>Municipio</span>
                        </label>
                    </div>
                </div>
                <div class="col-md-2" id="divchkNoSabe">
