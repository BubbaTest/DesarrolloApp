@Html.Hidden("txtProyecto", (object)ViewBag.Proyecto.ToString())
<div class="content">
    <div class="card">
        <div class="card-header">
            <h5 class="mb-0">Mantenimiento de Ambiente</h5>
        </div>
        <div class="card-body">
            <ul class="nav nav-tabs mb-3" id="pills-tab1" role="tablist">
                <li class="nav-item" id="tabDatosUsuario">
                    <a class="nav-link active" id="frUsuario-tab" data-toggle="pill" href="#frUsuario" role="tab" aria-controls="frUsuario" aria-selected="true">Usuario</a>
                </li>
                <li class="nav-item" id="tabDatosRol">
                    <a class="nav-link" id="frRoles-tab" data-toggle="pill" href="#frRoles" role="tab" aria-controls="frRoles" aria-selected="true">Roles</a>
                </li>
                <li class="nav-item" id="tabDatosOpcion">
                    <a class="nav-link" id="frOpcAso-tab" data-toggle="pill" href="#frOpcAso" role="tab" aria-controls="frOpcAso" aria-selected="true">Opciones</a>
                </li>
                <li class="nav-item" id="tabDatosCensoEncuesta">
                    <a class="nav-link" id="frCenEnc-tab" data-toggle="pill" href="#frCenEnc" role="tab" aria-controls="frCenEnc" aria-selected="true">Censo/Encuesta</a>
                </li>
            </ul>

            <div class="tab-content" id="pills-tabContent1">
                <div class="tab-pane fade show active" id="frUsuario" role="tabpanel" aria-labelledby="frUsuario-tab">
                    @Html.Partial("ListaUsuario")
                </div>
                <div class="tab-pane fade" id="frRoles" role="tabpanel" aria-labelledby="frRoles-tab">
                    @Html.Partial("ListaRol")
                </div>
                <div class="tab-pane fade" id="frOpcAso" role="tabpanel" aria-labelledby="frOpcAso-tab">
                    @Html.Partial("ListaOpcAso")
                </div>
                <div class="tab-pane fade" id="frCenEnc" role="tabpanel" aria-labelledby="frCenEnc-tab">
                    @Html.Partial("ListaCenEnc")
                </div>
            </div>
        </div>
    </div>
</div>
@using (Html.BeginScriptContext())
{
    Html.AddScriptBlock(
        @<script type="text/javascript">
            var vusuarioId;
            var vrolId;
            var vopcionId;
            var vId;
        </script>
    , true);
}

