<div class="content">
    <div class="card">
        <div class="card-header">
            <h5 class="mb-0">Mantenimiento de Variables Encuesta</h5>
        </div>
        <div class="card-body">
            <ul class="nav nav-tabs mb-3" id="pills-tab1" role="tablist">
                <li class="nav-item" id="tabDatosSeccion">
                    <a class="nav-link active" id="frSeccion-tab" data-toggle="pill" href="#frSeccion" role="tab" aria-controls="frSeccion" aria-selected="true">Sección</a>
                </li>
                <li class="nav-item" id="tabDatosVariable">
                    <a class="nav-link" id="frVariables-tab" data-toggle="pill" href="#frVariables" role="tab" aria-controls="frVariables" aria-selected="true">Variables</a>
                </li>
            </ul>

            <div class="tab-content" id="pills-tabContent1">
                <div class="tab-pane fade show active" id="frSeccion" role="tabpanel" aria-labelledby="frUsuario-tab">
                    @Html.Partial("ListaSeccion")
                </div>
                <div class="tab-pane fade" id="frVariables" role="tabpanel" aria-labelledby="frVariables-tab">
                   @Html.Partial("ListaVariable")
                </div>
            </div>
        </div>
    </div>
</div>
@using (Html.BeginScriptContext())
{
    Html.AddScriptBlock(
        @<script type="text/javascript">
            var vseccionId;
            var vvariableId;
        </script>
    , true);
}
